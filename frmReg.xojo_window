		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnOkay
	#tag Event
		Sub Action()
		  regName = txtName.Text
		  regCode = txtCode.text
		  if isRegGood = true then
		    MsgBox "Thank you for purchasing App-Make-inator!"
		    frmReg.close
		  else
		    Dim d as New MessageDialog  //declare the MessageDialog object
		    Dim b as MessageDialogButton //for handling the result
		    d.icon=MessageDialog.GraphicCaution   //display warning icon
		    d.ActionButton.Caption="OK"
		    'd.CancelButton.Visible=True     //show the Cancel button
		    d.AlternateActionButton.Visible=True   //show the "Don't Save" button
		    d.AlternateActionButton.Caption="Purchase"
		    d.Message="Registration code not valid."
		    d.Explanation="The registration code you have entered does not appear to be valid.  Please check that spelling and case for both the code and name match the information in your "+ _
		    "registration confirmation email."
		    
		    b=d.ShowModal     //display the dialog
		    Select Case b //determine which button was pressed.
		    Case d.ActionButton
		      //user pressed Save
		    Case d.AlternateActionButton
		      //user pressed Don't Save
		      ShowURL "http://www.theinators.com/buy.html"
		    Case d.CancelButton
		      //user pressed Cancel
		    End select
		  end if
		  modPrefs.WriteAllPrefs
		End Sub
	#tag EndEvent
#tag EndEvents
