#tag WindowCode
	#tag Event
		Sub Open()
		  txtName.Text = modPrefs.regName
		  txtCode.Text = modPrefs.regCode
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events RadLinkLabel1
