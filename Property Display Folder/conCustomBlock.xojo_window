#tag WindowCode
	#tag Event
		Sub Open()
		  if myObj.RequireMathVariable1 = true then
		    
		    lblVar1Description.Visible = true
		    lblVar1Description.Text = myObj.Var1Description
		    popVar1.Visible = true
		    
		    if myObj.Var1Type = "Text" then
		      popVar1.PopulateWithText
		    ElseIf myObj.Var1Type = "Number" then
		      popVar1.PopulateWithNumbers
		    ElseIf myObj.Var1Type = "Dictionary" then
		      popVar1.PopulateWithDicts
		    end if
		    
		    popVar1.SelectVariableByName(myObj.MathVariable1)
		    
		  else
		    
		    lblVar1Description.Visible = false
		    popVar1.Visible = false
		    
		  end if
		  
		  if myObj.RequireMathVariable2 = true then
		    
		    lblVar2Description.Visible = true
		    lblVar2Description.Text = myObj.Var2Description
		    popVar2.Visible = true
		    
		    if myObj.Var2Type = "Text" then
		      popVar2.PopulateWithText
		    ElseIf myObj.Var2Type = "Number" then
		      popVar2.PopulateWithNumbers
		    ElseIf myObj.Var2Type = "Dictionary" then
		      popVar2.PopulateWithDicts
		    end if
		    
		    popVar2.SelectVariableByName(myObj.MathVariable2)
		    
		  else
		    
		    lblVar2Description.Visible = false
		    popVar2.Visible = false
		    
		  end if
		  
		  
		  if myObj.RequireMathVariable3 = true then
		    
		    lblVar3Description.Visible = true
		    lblVar3Description.Text = myObj.Var3Description
		    popVar3.Visible = true
		    
		    if myObj.Var3Type = "Text" then
		      popVar3.PopulateWithText
		    ElseIf myObj.Var3Type = "Number" then
		      popVar3.PopulateWithNumbers
		    ElseIf myObj.Var3Type = "Dictionary" then
		      popVar3.PopulateWithDicts
		    end if
		    
		    popVar3.SelectVariableByName(myObj.MathVariable3)
		    
		  else
		    
		    lblVar3Description.Visible = false
		    popVar3.Visible = false
		    
		  end if
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function myObj() As ilObjectCustomBlock
		  return ilObjectCustomBlock(frmProject.chartProject.selectedFrame.tag)
		  
		End Function
	#tag EndMethod


