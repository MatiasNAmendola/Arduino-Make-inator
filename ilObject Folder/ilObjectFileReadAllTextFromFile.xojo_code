Inherits ilObjectBase
	#tag Event
		Sub GenerateAndroidCode(theOrigin as string)
		  dim s as string
		  
		  s = "public void " + GetCodeName + "()" + EndOfLine
		  
		  s = s + "{" + EndOfLine
		  
		  'try {
		  'BufferedReader in = new BufferedReader(new FileReader("infilename"));
		  'String str;
		  'while ((str = in.readLine()) != null) {
		  ///'process(str);
		  '}
		  'in.close();
		  '} catch (IOException e) {
		  '}
		  
		  
		  ////////// or
		  
		  
		  
		  
		  s = s + "try {" + EndOfLine
		  s = s + "InputStream in = openFileInput(""" + modVariables.VariableObjectFromName(MathVariable1).ISCClassLocalAndroidName + """);" + EndOfLine
		  s = s + "if (in != null) {" + EndOfLine
		  s = s + "InputStreamReader inputreader = new InputStreamReader(in);" + EndOfLine
		  s = s + "BufferedReader buffreader = new BufferedReader(inputreader);" + EndOfLine
		  s = s + "String line;" + EndOfLine
		  s = s + "while ((line = buffreader.readLine()) != null) {" + EndOfLine
		  s = s + "}" + EndOfLine
		  
		  s = s + modVariables.VariableObjectFromName(OriginVariable).ISCClassLocalAndroidName + " = " + modVariables.VariableObjectFromName(OriginVariable).ISCClassLocalAndroidName + " + line;" + EndOfLine
		  
		  s = s + "}" + EndOfLine
		  s = s + "in.close();" + EndOfLine
		  s = s + "} catch (java.io.FileNotFoundException e) {" + EndOfLine
		  s = s + "} catch (IOException e) {" + EndOfLine
		  s = s + "e.printStackTrace();" + EndOfLine
		  s = s + "}" + EndOfLine
		  
		  
		  
		  
		  s = s + "//" + GetCodeName + "Done" + EndOfLine
		  
		  s = s + "}" + EndOfLine + EndOfLine
		  
		  setCodeToCallThisObjectForName("Read", GetCodeName + "();")
		  
		  AndroidInsertCodeAtMarker(s, "//EndOfFunctions")
		End Sub
	#tag EndEvent

	#tag Event
